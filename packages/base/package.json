  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@metamask/auto-changelog": "^3.4.4",
    "eslint": "^9.11.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-x": "^4.3.0",
    "eslint-plugin-jsdoc": "^50.2.4",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "prettier": "^3.3.3"
  },
  "peerDependencies": {
    "eslint": "^9.11.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-x": "^4.3.0",
    "eslint-plugin-jsdoc": "^50.2.4",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": "^18.18 || >=20"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@eslint/js": "^9.11.0",
    "globals": "^15.9.0"
  }
