        "types": "./src/index.d.mts",
        "default": "./src/index.mjs"
      }
    }
  },
  "files": [
    "src/",
    "!src/**/*.test.js"
  ],
  "scripts": {
    "lint:changelog": "auto-changelog validate",
    "publish": "npm publish",
    "test": "eslint ."
